@using TravelTripProje.Models.Siniflar
@model List<Blog>


<div class="padding">
    <h4>En Popüler 10 Blog</h4>

    <ul class="list list-pad">
        @{int i = 0;}
        @foreach (var item in Model)
        {
        <li class="list-bg"><span>@{i++;}@i</span><a href="/Blog/BlogDetay/@item.ID">@item.Baslik</a></li>
        }
    </ul>
    <a href="/Blog/Index/" class="link1">Diğer Yerler</a>
</div>